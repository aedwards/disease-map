	<s:states>
		<s:State enterState="selectAllCountries()" name="ViewByCountry"/>
		<s:State enterState="selectAllDiseases()" name="ViewByDisease"/>
		<s:State name="HHSAndOverlap"/>
		<s:State name="USDASelectAgents"/>
		<s:State name="Anthrax"/>
	</s:states>
	
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
		<s:RadioButtonGroup id="categorySelect"/>
		<s:RadioButtonGroup id="ViewByDiseaseGroup"/>
	</fx:Declarations>
	
	
	
	<fx:Script>
		<![CDATA[
			import com.ammap.ammap_com_internal;
			
			import flash.events.Event;
			import flash.events.KeyboardEvent;
			import flash.events.MouseEvent;
			import flash.text.TextFormat;
			import flash.ui.Keyboard;
			import mx.managers.PopUpManager;		//import the specific manager for our component
			import components.DirectionsWindow;		//import the custom component
			
			/*IMPORTANT:: The ViewByDisease State is the name for the Category A1 selection.  The ViewByCountry State is the name for the All selection.  
			*/
		
			
			[Bindable]
			[Embed(source="../assets/maps/world.swf")]	//embed and declare a private property for the map
			private var _world:Class;
			
			
			[Bindable]
			[Embed(source="../assets/icons/target.swf")]	//embed the target swf graphic and declare its private property
			private var _target:Class;
			
			[Bindable]
			private var _format:TextFormat;
			
			
			
			//This function just handles when the user clicks the Directions button.
			private function directionsPopUp(event:MouseEvent):void {
				PopUpManager.createPopUp(this, DirectionsWindow, true);
			}

			
			
			
			//functions that changes the state based on the selected radio button.  They are assigned to the click handlers of the respective radio buttons
			
			//this function is for the All Elements state radio button
			private function allAgents():void{
				if(this.currentState == "ViewByDisease" || this.currentState == "HHSAndOverlap" || this.currentState == "USDASelectAgents" || this.currentState == "Anthrax"){
					this.currentState = "ViewByCountry";
					rdAllElements.selected = true;
					rdAllElements.enabled = false;
					unCheck();	//uncheck all the diseases
					rdCategoryA1.selected = false;
					rdCategoryA1.enabled = true;
					rdHHSOverlap.selected = false;
					rdHHSOverlap.enabled = true;
					rdUSDAAgents.selected = false;
					rdUSDAAgents.enabled = true;
					rdAnthrax.selected = false;
					rdAnthrax.enabled = true;
					
				}
			}
			
			//this function is for the Category A1 state radio button
			private function changeToState2():void {
				if(this.currentState == "ViewByCountry" || this.currentState == "HHSAndOverlap" || this.currentState == "USDASelectAgents" || this.currentState == "Anthrax"){
					this.currentState = "ViewByDisease";
					rdCategoryA1.selected = true;
					rdCategoryA1.enabled = false;
					unCheck();	//uncheck all the diseases
					categoryA1Selections();
					tglToggleDiseaseCheckboxes.selected = true;
					rdAllElements.selected = false;
					rdAllElements.enabled = true;
					rdHHSOverlap.selected = false;
					rdHHSOverlap.enabled = true;
					rdUSDAAgents.selected = false;
					rdUSDAAgents.enabled = true;
					rdAnthrax.selected = false;
					rdAnthrax.enabled = true;
					dynamicLabelHeader.text = "List of Category A1 diseases.";
				}
			}
			
			//this function is for the HHS and Overlap state radio button
			private function changeToState3():void {
				if(this.currentState == "ViewByCountry" || this.currentState == "ViewByDisease" || this.currentState == "Anthrax" || this.currentState == "USDASelectAgents"){
					this.currentState = "HHSAndOverlap";
					rdHHSOverlap.selected = true;
					rdHHSOverlap.enabled = false;
					unCheck();	//uncheck all the diseases
					HHSOverlapSelections();
					tglToggleDiseaseCheckboxes.selected = true;
					rdAllElements.selected = false;
					rdAllElements.enabled = true;
					rdCategoryA1.selected = false;
					rdCategoryA1.enabled = true;
					rdUSDAAgents.selected = false;
					rdUSDAAgents.enabled = true;
					rdAnthrax.selected = false;
					rdAnthrax.enabled = true;
					dynamicLabelHeader.text = "List of diseases that fall into the HHS and Overlap category.";
				}
			}
			
			//this function is for the USDA Select Agents state radio button
			private function changeToState4():void {
				if(this.currentState == "ViewByCountry" || this.currentState == "ViewByDisease" || this.currentState == "HHSAndOverlap" || this.currentState == "Anthrax"){
					this.currentState = "USDASelectAgents";
					rdUSDAAgents.selected = true;
					rdUSDAAgents.enabled = false;
					unCheck();	//uncheck all the diseases
					usdaSelectAgentsSelections();
					tglToggleDiseaseCheckboxes.selected = true;
					rdAllElements.selected = false;
					rdAllElements.enabled = true;
					rdCategoryA1.selected = false;
					rdCategoryA1.enabled = true;
					rdHHSOverlap.selected = false;
					rdHHSOverlap.enabled = true;
					rdAnthrax.selected = false;
					rdAnthrax.enabled = true;
					dynamicLabelHeader.text = "List of USDA Select Agent diseases.";
				}
			}
			
			//this function is for the Anthrax state radio button
			private function changeToState5():void {
				if(this.currentState == "ViewByCountry" || this.currentState == "ViewByDisease" || this.currentState == "HHSAndOverlap" || this.currentState == "USDASelectAgents"){
					this.currentState = "Anthrax";
					rdAnthrax.selected = true;
					rdAnthrax.enabled = false;
					unCheck();	//uncheck all the diseases
					anthraxSelections();
					copyReveal();
					tglToggleDiseaseCheckboxes.selected = true;
					rdAllElements.selected = false;
					rdAllElements.enabled = true;
					rdCategoryA1.selected = false;
					rdCategoryA1.enabled = true;
					rdHHSOverlap.selected = false;
					rdHHSOverlap.enabled = true;
					rdUSDAAgents.selected = false;
					rdUSDAAgents.enabled = true;
					dynamicLabelHeader.text = "Anthrax disease.";
				}
				
			}
			
			
			
			//this function handles the "Toggle Selections" button which selects all checkboxes in the current state when clicked.
			private function selectAllCheckboxes():void {
				var idx:uint;
				if (this.currentState == "ViewByCountry") {
					var countryLength:uint = viewByCountryGroup.numElements;
					for(idx = 0; idx < countryLength; idx++) {
						var elementCountry:CheckBox = viewByCountryGroup.getElementAt(idx) as CheckBox;
						elementCountry.selected = tglToggleCountryCheckboxes.selected;
					}
				}
				else if(this.currentState == "ViewByDisease" || this.currentState == "HHSAndOverlap" || this.currentState == "USDASelectAgents" || this.currentState == "Anthrax") {
					var len:uint = viewByDiseaseGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementDiseases:CheckBox = viewByDiseaseGroup.getElementAt(idx) as CheckBox;
						elementDiseases.selected = tglToggleDiseaseCheckboxes.selected;
					}
				}
			}
			
			/*this function applies to the Countries|Diseases toggle button in the ViewByCountry State. it allows the user
			 * to change the between a list of countries or a list of diseases.
			 * Note: There are 2 "Toggle Selctions" buttons, one layered on top of the other in this order in Design mode: 
			 * tglToggleDiseaseCountryState is on top of tglToggleCountryCheckboxes.
			 */
			private function switchSelection():void {
				if (this.currentState == "ViewByCountry" && viewByDiseaseGroup.visible == false && viewByDiseaseScroller.visible == false) {
					tglToggleCountryCheckboxes.selected = false;
					tglToggleCountryCheckboxes.visible = false;
					tglToggleDiseaseCountryState.selected = true;
					tglToggleDiseaseCountryState.visible = true;
					selectAllDiseases2();
					unCheckCountries();
					viewByDiseaseGroup.visible = true;
					viewByDiseaseScroller.visible = true;
					viewByCountryGroup.visible = false;
					groupCountryScroller.visible = false;
					dynamicCountriesDiseasesHeader.text = "List of Diseases";
					countryDiseaseLabelHeader.text = "Select your desired disease.";
				}
				else if (this.currentState == "ViewByCountry") {
					tglToggleCountryCheckboxes.selected = true;
					tglToggleCountryCheckboxes.visible = true;
					selectAllCountries();
					unCheckDiseases();
					tglToggleDiseaseCountryState.selected = false;
					tglToggleDiseaseCountryState.visible = false;
					viewByCountryGroup.visible = true;
					groupCountryScroller.visible = true;
					viewByDiseaseGroup.visible = false;
					viewByDiseaseScroller.visible = false;
					dynamicCountriesDiseasesHeader.text = "List of Countries";
					countryDiseaseLabelHeader.text = "Select your desired country.";
				}
			}
			
			
			//this selects all the country checkboxes when the View by Country state is in focus.  It is also the enterState for the ViewByCountry State.
			private function selectAllCountries():void {
				viewByCountryGroup.visible = true;
				groupCountryScroller.visible = true;
				viewByDiseaseGroup.visible = false;
				viewByDiseaseScroller.visible = false;
				tglToggleCountryCheckboxes.selected = true;
				tglToggleCountryCheckboxes.visible = true;
				tglToggleDiseaseCountryState.selected = false;
				tglToggleDiseaseCountryState.visible = false;
				var idx:uint;
				if(this.currentState == "ViewByCountry") {
					var countryLength:uint = viewByCountryGroup.numElements;
					for(idx = 0; idx < countryLength; idx++) {
						var elementCountry:CheckBox = viewByCountryGroup.getElementAt(idx) as CheckBox;
						elementCountry.selected = tglToggleCountryCheckboxes.selected;
					}	
				}
				dynamicCountriesDiseasesHeader.text = "List of Countries";
				countryDiseaseLabelHeader.text = "Select your desired country.";
			}
			
			//this selects all the disease checkboxes when the View by Disease state is in focus
			private function selectAllDiseases():void {
				tglToggleDiseaseCheckboxes.selected= true;
				if(this.currentState == "ViewByDisease" || this.currentState == "ViewByCountry") {
					var idx:uint;
					
					var len:uint = viewByDiseaseGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementDiseases:CheckBox = viewByDiseaseGroup.getElementAt(idx) as CheckBox;
						elementDiseases.selected = true;
					}
				}
			}
			
			//this selects all the disease checkboxes when the ViewByCountry state is in focus and the user has toggled the menu selection from Countries to Diseases
			private function selectAllDiseases2():void {
				
				if(this.currentState == "ViewByCountry") {
					var idx:uint;
					var len:uint = viewByDiseaseGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementDiseases:CheckBox = viewByDiseaseGroup.getElementAt(idx) as CheckBox;
						elementDiseases.selected = tglToggleDiseaseCountryState.selected;
					}
				}
			}
			
			//deselects all the checkboxes for all the diseases so that the checks do not stay persistent through states.
			private function unCheck():void {
				if(this.currentState !== "ViewByDisease" || this.currentState == "ViewByDisease") {
					var idx:uint;
					tglToggleDiseaseCheckboxes.selected = false;
					tglToggleCountryCheckboxes.selected = false;
					var len:uint = viewByDiseaseGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementDiseases:CheckBox = viewByDiseaseGroup.getElementAt(idx) as CheckBox;
						elementDiseases.selected = false;
					}
				}	
			}
			
			//deselects all the checkboxes for all the diseases when the menu context in the ViewByCountry state is switched to view countries.
			private function unCheckDiseases():void {
				if(this.currentState == "ViewByCountry") {
					var idx:uint;
					tglToggleDiseaseCountryState.selected = false;
					tglToggleCountryCheckboxes.selected = true;
					var len:uint = viewByDiseaseGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementDiseases:CheckBox = viewByDiseaseGroup.getElementAt(idx) as CheckBox;
						elementDiseases.selected = tglToggleDiseaseCountryState.selected;
					}
				}	
			}
			
			//deselects all the checkboxes for all the countries when the menu context in the ViewByCountry state is switched to view diseases.
			private function unCheckCountries():void {
				if(this.currentState == "ViewByCountry") {
					var idx:uint;
					tglToggleDiseaseCountryState.selected = true;
					tglToggleCountryCheckboxes.selected = false;
					var len:uint = viewByCountryGroup.numElements;
					for(idx = 0; idx < len; idx++) {
						var elementCountries:CheckBox = viewByCountryGroup.getElementAt(idx) as CheckBox;
						elementCountries.selected = tglToggleCountryCheckboxes.selected;
					}
				}	
			}
			
			/*the following functions just check the desired checkboxes in order to correspond to the respective
			diseases that show up in each category.  It's mainly just a visual cue for the user to know which diseases are showing up on the map
			for that specific category.  Other checkboxes can be added if desired. 
			*/
			private function categoryA1Selections():void {
				viewByDiseaseGroup.visible = true;
				viewByDiseaseScroller.visible = true;
				chkYersiniaPestis.selected = true;
				chkEbolaViruses.selected = true;
				chkFrancisellaT.selected = true;
				chkLassaFever.selected = true;
				chkMarburgVirus.selected = true;
				chkBacillusAnthracis.selected = true;
			}
			
			private function HHSOverlapSelections():void {
				viewByDiseaseGroup.visible = true;
				viewByDiseaseScroller.visible = true;
				chkCoccidioidesIP.selected = true;
				chkEhrlichiaRuminantium.selected = true;
				chkCongoH.selected = true;
				chkBacillusAnthracis.selected = true;
				chkBrucellaAbortus.selected = true;
				chkBrucellaMelitensis.selected = true;
				chkBrucellaSuis.selected = true;
				chkBurkholderiaPseudo.selected = true;
				chkBurkholderiaMallei.selected = true;
				chkEasternEquine.selected = true;
				chkHendraVirus.selected = true;
				chkNipahVirus.selected = true;
				chkRiftValleyFever.selected = true;
				chkVenezuelanEquineE.selected = true;
				chkMonkeypoxVirus.selected = true;
				chkLassaFever.selected = true;
				chkMarburgVirus.selected = true;
				chkRockyMountainFever.selected = true;
				chkTickEncephalitisComplex.selected = true;
				chkRickettsia.selected = true;
				chkYersiniaPestis.selected = true;
			}
			
			private function usdaSelectAgentsSelections():void {
				viewByDiseaseGroup.visible = true;
				viewByDiseaseScroller.visible = true;
				chkAfricanHorseSickness.selected = true;
				chkAfricanSwineFever.selected = true;
				chkAvianInfluenza.selected = true;
				chkBlueTongue.selected = true;
				chkBovineSpongiform.selected = true;
				chkCamelPox.selected = true;
				chkClassicSwineFever.selected = true;
				//chkCowdriaRuminatium.selected = true;
				chkEhrlichiaRuminantium.selected = true;
				chkEncephalitisVirus.selected = true;
				chkFootAndMouth.selected = true;
				chkGoatPox.selected = true;
				chkLumpySkin.selected = true;
				chkMalignantCatarrhal.selected = true;
				chkMenangleVirus.selected = true;
				chkMycoplasmaCp.selected = true;
				chkMycoplasmaM.selected = true;
				chkPesteR.selected = true;
				//Rinderpest virus eradicated
				chkRiftValleyFever.selected = true;
				chkRockyMountainFever.selected = true;
				chkSheepPox.selected = true;
				chkSwineVesicular.selected = true;
				chkVesicularStomatitis.selected = true;
				chkNewcastleDisease.selected = true;
			}
			
			private function anthraxSelections():void {
				viewByDiseaseGroup.visible = true;
				viewByDiseaseScroller.visible = true;
				chkBacillusAnthracis.selected = true;
			}
			
			private function copyReveal():void {
				if (this.currentState == "Anthrax") {
					this.stage.addEventListener(KeyboardEvent.KEY_DOWN, this.onKeyDown);
					this.stage.addEventListener(KeyboardEvent.KEY_UP, this.onKeyUp);
				}
				else {
					this.stage.removeEventListener(KeyboardEvent.KEY_DOWN, this.onKeyDown);
					this.stage.removeEventListener(KeyboardEvent.KEY_UP, this.onKeyUp);
				}
			
			}
			
			private function onKeyDown(event:KeyboardEvent):void {
				
				if (event.keyCode == Keyboard.SHIFT && tglToggleDiseaseCheckboxes.selected == false && worldMap.zoomLevel == 200) {
					copyrightLabel.visible = true;
				}
				else {
					copyrightLabel.visible = false;
				}
				
			}
			
			private function onKeyUp(event:KeyboardEvent):void {
				copyrightLabel.visible = false;
			}
			
			
			
		]]>
	</fx:Script>
	
	
	
